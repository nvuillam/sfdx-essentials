{
  "filters": [
    {
      "name": "ProfileFiltering",
      "description": "Remove unwanted stuff in profiles",
      "folders": ["profiles"],
      "file_extensions": ["profile"],
      "exclude_list": [
        {
          "type_tag": "userPermissions",
          "identifier_tag": "name",
          "values": ["AllowUniversalSearch", "EnableNotifications"]
        },
        {
          "type_tag": "tabVisibilities",
          "identifier_tag": "tab",
          "values": ["TabExportScripts", "TabInstallScripts"]
        }
      ]
    }
  ]
}
