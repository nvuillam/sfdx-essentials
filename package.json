{
  "name": "sfdx-essentials",
  "description": "Tools to cover the lacks of missing required functions of SFDX",
  "version": "0.1.3",
  "author": "NicolasVuillamy @nvuillam",
  "bugs": "https://github.com/nvuillam/sfdx-essentials/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "^1.6.33",
    "@oclif/errors": "1",
    "@salesforce/command": "0.1.5",
    "camelcase": "^5.0.0",
    "cint": "^8.2.1",
    "fs": "0.0.1-security",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.2",
    "path": "^0.12.7",
    "read-each-line-sync": "^1.0.5",
    "util": "^0.11.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.15.3",
    "@oclif/plugin-help": "1",
    "@oclif/test": "1",
    "@salesforce/dev-config": "1.0.4",
    "@types/chai": "4",
    "@types/jsforce": "1.8.9",
    "@types/mocha": "^5.2.3",
    "@types/node": "9",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "sinon": "5",
    "ts-node": "5",
    "tslib": "^1.9.3",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/nvuillam/sfdx-essentials",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "GPL-3.0-or-later",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "nvuillam/sfdx-essentials",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
